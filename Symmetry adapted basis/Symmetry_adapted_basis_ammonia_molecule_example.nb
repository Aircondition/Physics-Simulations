Cell[TextData[{
 "The symmetry adapted basis is important because, in this basis the group \
representation is in block-diagonal form. The function ",
 StyleBox["operatorReduce",
  FontWeight->"Bold"],
 " writes a matrix ",
 StyleBox["operator",
  FontWeight->"Bold",
  FontSlant->"Italic"],
 " in the basis specified by ",
 StyleBox["basis",
  FontWeight->"Bold",
  FontSlant->"Italic"],
 "."
}], "Text",
 CellChangeTimes->{{3.850470173268693*^9, 
  3.8504703160724497`*^9}},ExpressionUUID->"e8e9c3a2-9f05-45bb-9e76-\
9a2715734886"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"operatorReduce", "[", 
    RowBox[{"operator_", ",", "basis_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"image", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"operator", ".", "#"}], ")"}], "&"}], "/@", "basis"}]}], 
       ",", 
       RowBox[{"out", "=", 
        RowBox[{"{", "}"}]}], ",", "a"}], "}"}], ",", 
     RowBox[{"Transpose", "@", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Flatten", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"a", "[", "j", "]"}], ",", 
             RowBox[{"{", 
              RowBox[{"j", ",", 
               RowBox[{"Length", "@", "basis"}]}], "}"}]}], "]"}], "/.", 
           RowBox[{"Solve", "[", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{
              RowBox[{"Sum", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"a", "[", "k", "]"}], 
                 RowBox[{"basis", "[", 
                  RowBox[{"[", "k", "]"}], "]"}]}], ",", 
                RowBox[{"{", 
                 RowBox[{"k", ",", 
                  RowBox[{"Length", "@", "basis"}]}], "}"}]}], "]"}], 
              "\[Equal]", 
              RowBox[{"image", "[", 
               RowBox[{"[", "i", "]"}], "]"}]}], ",", 
             RowBox[{"Table", "[", 
              RowBox[{
               RowBox[{"a", "[", "k", "]"}], ",", 
               RowBox[{"{", 
                RowBox[{"k", ",", "1", ",", 
                 RowBox[{"Length", "@", "basis"}]}], "}"}]}], "]"}]}], 
            "\[IndentingNewLine]", "]"}]}], ",", "2"}], "]"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{"i", ",", 
          RowBox[{"Length", "@", "basis"}]}], "}"}]}], "]"}]}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"DirectSum", "[", 
   RowBox[{
    RowBox[{"a_List", "?", "MatrixQ"}], ",", 
    RowBox[{"b_List", "?", "MatrixQ"}]}], "]"}], ":=", 
  RowBox[{"ArrayFlatten", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"a", ",", "0"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"0", ",", "b"}], "}"}]}], "}"}], "]"}], 
  RowBox[{"(*", " ", 
   RowBox[{"Direct", " ", "sum", " ", "of", " ", "matrices", " ", 
    StyleBox["a",
     FontSlant->"Italic"], " ", "and", " ", 
    StyleBox["b",
     FontSlant->"Italic"]}], " ", "*)"}]}]}], "Input",
